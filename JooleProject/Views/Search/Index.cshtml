@model IEnumerable<JooleProject.Models.tblCategory>

@{
    ViewBag.Title = "Index";
}

<h2>Search</h2>


@using (Html.BeginForm("Index", "GenerateInvoice", FormMethod.Get))
{
    @Html.AntiForgeryToken()

    @Html.DropDownList("CatID", (SelectList)ViewBag.catselect, "Select Category", new { @class = "form-control" })

}

<input type="search" id="site-search" name="q" aria-label="Search through products">
<input type="button" value="Search" placeholder="Search..." onclick="location.href='@Url.Action("SearchResult", "SearchResult")'" />
